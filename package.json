{
    "name": "fire-ui",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/epycsolutions/fire-ui-react.git"
    },
    "author": "EpycSolutions",
    "license": "MIT",
    "scripts": {
        "prepare": "husky install",
        "clean:emotion-types": "rimraf node_modules/@emotion/core/types",
        "postinstall": "npm clean:emotion-types",
        "build:tokens": "fire-cli token packages/components/theme/src/index.ts --out packages/cre/styled-system/dist/theming.types.d.ts",
        "build": "turbo run build --filter=!./examples/* --filter=!*props-docs",
        "build:fast": "turbo run build:fast --filter=!./examples/* --filter=!*props-docs",
        "build:prop-docs": "npm typedocs && npm --filter=*props-docs build",
        "typedocs": "npm --filter*./packages/components/** typedocs",
        "postbuild": "npm build:tokens",
        "dev": "turbo run dev --filter=!./examples/*",
        "typecheck": "tsc --noEmit",
        "build:theme": "cd packages/components/theme && npm build:fast",
        "test": "jest",
        "lint": "eslint packages/**/**/src --ext .ts,.tsx --config .eslintrc --cache",
        "format:check": "prettier --check packages/**/**/src --cache",
        "format:write": "prettier --write packages/**/**/src --cache",
        "build:addon": "cd tooling/storybook-addon && npm build",
        "clean:storybook": "rm -rf node_modules/.cache/storybook",
        "storybook": "npm build:addon && storybook dev -p 6006",
        "build:storybook": "npm build:addon && storybook build",
        "serve:storybook": "npx serve storybook-static",
        "clean": "npm -r --parallel exec rimraf dist .turbo *.log",
        "changelog": "tsx scripts/changelog.ts",
        "changelog:latest": "tsx scripts/changelog.ts --latest",
        "changelog:commit": "git add . && git commit -am 'docs: add changelog' && git push",
        "changelog:format": "prettier --write .changelog",
        "csb": "tsx scripts/csb.ts",
        "create:package": "plop component",
        "version": "changeset version",
        "release": "changeset publish",
        "version:dev": "changeset version --snapshot dev",
        "release:dev": "changeset publish --tag dev",
        "sandbox": "tsx scripts/sandbox.ts",
        "notify:release": "npm-run-all changelog:write changelog:commit"
    },
    "version": "0.0.0",
    "dependencies": {
        "@babel/core": "7.22.10",
        "@babel/eslint-parser": "7.22.10",
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/runtime": "7.22.10",
        "@changesets/changelog-github": "0.4.8",
        "@changesets/cli": "2.26.2",
        "@changesets/get-release-plan": "3.0.17",
        "@changesets/types": "5.2.1",
        "@commitlint/cli": "17.7.1",
        "@commitlint/config-conventional": "17.7.0",
        "@octokit/rest": "20.0.1",
        "@storybook/addon-a11y": "7.2.3",
        "@storybook/addon-essentials": "7.2.3",
        "@storybook/addon-storysource": "7.2.3",
        "@storybook/addons": "7.2.3",
        "@storybook/cli": "7.2.3",
        "@storybook/react": "7.2.3",
        "@storybook/react-vite": "7.2.3",
        "@storybook/theming": "7.2.3",
        "@swc-node/jest": "1.6.6",
        "@swc/core": "1.3.76",
        "@testing-library/jest-dom": "5.17.0",
        "@types/edit-json-file": "1.7.0",
        "@types/fs-extra": "11.0.1",
        "@types/jest": "29.5.3",
        "@types/mkdirp": "2.0.0",
        "@types/node": "20.4.10",
        "@types/react": "18.2.20",
        "@types/react-dom": "18.2.7",
        "@types/testing-library__jest-dom": "5.14.9",
        "@typescript-eslint/eslint-plugin": "6.3.0",
        "@typescript-eslint/parser": "6.3.0",
        "dotenv": "16.3.1",
        "edit-json-file": "1.7.0",
        "eslint": "8.47.0",
        "eslint-config-airbnb-typescript": "17.1.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-config-react": "1.1.7",
        "eslint-plugin-import": "2.28.0",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-react": "7.33.1",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "6.0.0",
        "find-packages": "10.0.4",
        "find-up": "6.3.0",
        "fs-extra": "11.1.1",
        "husky": "8.0.3",
        "jest": "29.6.2",
        "jest-environment-jsdom": "29.6.2",
        "jest-watch-typeahead": "2.2.2",
        "lint-staged": "13.2.3",
        "node-fetch": "3.3.2",
        "npm-run-all": "4.1.5",
        "ora": "7.0.1",
        "outdent": "0.8.0",
        "plop": "3.1.2",
        "prettier": "3.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rimraf": "5.0.1",
        "storybook": "7.2.3",
        "tsup": "7.2.0",
        "tsx": "3.12.7",
        "turbo": "1.10.12",
        "typescript": "5.1.6",
        "unbuild": "1.2.1",
        "vite": "4.4.9"
    },
    "engines": {
        "node": ">=16.x"
    },
    "devDependencies": {
        "@babel/preset-env": "7.22.10",
        "@babel/preset-react": "7.22.5",
        "@babel/preset-typescript": "7.22.5"
    }
}